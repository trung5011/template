				script(type='text/javascript').
					jQuery(document).ready(function($) {
					/*search vehicle*/
					$('.js-search').keyup(function() {
					event.preventDefault();
					var this_el = $(this);
					var this_val = this_el.val();
					$.ajax({
					url: wp_vars.ADMIN_AJAX_URL,
					type: 'post',
					dataType: 'json',
					data: {
					title: this_val,
					nonce: "22fde32c3c",
					action: "get_projects_title_ajax"
					}
					})
					.done(function(response) {
					if (response.success) {
					$('ul.frm_suggest-info').html(response.data);
					}
					})
					.fail(function() {
					console.log("LOAD DATA ERROR!");
					});
					});
					var _setTimeout;
					var _clearTimeout;
					var _pauseOn;
					_setTimeout = window.setTimeout;
					_clearTimeout = window.clearTimeout;
					window.setTimeout = function(code, delay) {
					return _setTimeout(function() {
					if (_pauseOn == true) {
					return;
					}
					code();
					}, delay);
					};
					window.clearTimeout = function(intervalId) {
					if (_pauseOn == true) {
					return;
					}
					_clearTimeout(intervalId);
					};
					function pauseTimer() {
					_pauseOn = true;
					}
					var typed = new Typed("#js-typed-search", {
					stringsElement: '#typed-strings',
					typeSpeed: 80,
					backSpeed: 50,
					backDelay: 500,
					startDelay: 0,
					loop: true,
					fadeOut: true,
					fadeOutClass: 'typed-fade-out',
					fadeOutDelay: 1000,
					});
					$("#js-typed-search").click(function() {
					pauseTimer();
					$("#js-typed-search").val('');
					});
					});
			.section
