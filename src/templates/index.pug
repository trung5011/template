						// ajax handler
						script.
							jQuery(document).ready(function($) {
							$("#select-subdivision").selectmenu();
							var formButton = $('#contact_form_submit').find('button');
							var check_boolean_name = false;
							$('#focus-name').keyup(function(event) {
							var name = $(this).val();
							var name_msg = $('body').find('.name_msg');
							if (hasNumber(name)) {
							name_msg.text('Họ và tên liên hệ không được nhập số !');
							formButton.css("cursor", "not-allowed");
							formButton.prop('disabled', true);
							check_boolean_name = false;
							} else {
							name_msg.text('');
							formButton.css("cursor", "pointer");
							formButton.prop('disabled', false);
							check_boolean_name = true;
							}
							});
							var check_boolean_email = false;
							$('#focus-email').change(function(event) {
							var email = $(this).val();
							var email_msg = $('body').find('.email_msg');
							if (!validateEmail(email) && email) {
							email_msg.text('Email không hợp lệ !');
							formButton.css("cursor", "not-allowed");
							formButton.prop('disabled', true);
							check_boolean_email = false;
							} else {
							email_msg.text('');
							formButton.css("cursor", "pointer");
							formButton.prop('disabled', false);
							check_boolean_email = true;
							}
							});
							var check_boolean_phone = false;
							$('#focus-phone').change(function(event) {
							var phone_text = $(this).val();
							var count_number = phone_text.length;
							var phone_msg = $('body').find('.phone_msg');
							if (!validatePhonenumber(phone_text) && phone_text || count_number > 11) {
							phone_msg.text('Số điện thoại không hợp lệ !');
							formButton.css("cursor", "not-allowed");
							formButton.prop('disabled', true);
							check_boolean_phone = false;
							} else {
							phone_msg.text('');
							formButton.css("cursor", "pointer");
							formButton.prop('disabled', false);
							check_boolean_phone = true;
							}
							});
							/*ajax handle submit form*/
							$('#contact_form_submit').submit(function(event) {
							event.preventDefault();
							var form = $(this);
							var form_button = document.getElementById('send_text');
							var form_button_text = form_button.innerHTML;
							var form_message = form.find('.message');
							if (check_boolean_phone) {
							$.ajax({
							url: wp_vars.ADMIN_AJAX_URL,
							type: 'post',
							dataType: 'json',
							data: form.serialize(),
							beforeSend: function() {
							form_button.innerHTML = "Gửi...";
							$('body').find('.fa-paper-plane').hide();
							},
							complete: function() {
							form_button.innerHTML = form_button_text;
							$('body').find('.fa-paper-plane').show();
							}
							})
							.done(function(response) {
							if (response.success) {
							form.find('.form-group').hide();
							form.find('.library-frm_submit').hide();
							$('body').find('.hide_frm_quote').hide();
							$current_url = "https://vincity.com";
							window.location.href = "" + '#thank_you';
							}
							if (response.msg) {
							form_message.html(response.msg);
							}
							})
							.fail(function() {
							console.log("Error!");
							});
							} else {
							console.log('Chưa thỏa mãn');
							}
							});
							});
						script(type='text/javascript').
							jQuery(document).ready(function($) {
							// select-subdivision
							$("#select-subdivision").selectmenu();
							/* count down charactor in content */
							$('#contact_content').keyup(function(event) {
							var this_val = $(this).val();
							var content_length = this_val.length;
							var count_down = 200 - content_length;
							document.getElementById('max_charactor').innerHTML = count_down;
							});
							});
